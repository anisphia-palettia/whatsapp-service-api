generator client {
  provider = "prisma-client-js"
  output   = "./generated"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Client {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String   @unique
  isActive  Boolean  @default(false)
  createdAt DateTime @default(now())

  clientKey String @unique @default(uuid())
}
